; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Capstone"
run/main_scene="res://common/UI/LoginScreen.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Game="*res://autoload/Game.gd"
Config="*res://autoload/Config.gd"
Save="*res://autoload/Save.gd"
Api="*res://autoload/Api.gd"
AuthController="*res://autoload/AuthController.gd"
GameTime="*res://autoload/GameTime.gd"
Audio="*res://autoload/Audio.gd"
Analytics="*res://autoload/Analytics.gd"
Accessibility="*res://autoload/Accessibility.gd"
ScreenShake="*res://autoload/ScreenShake.gd"
DifficultyManager="*res://autoload/DifficultyManager.gd"
TransitionManager="*res://autoload/TransitionManager.gd"
PerformanceMonitor="*res://common/Util/PerformanceMonitor.gd"
ZoneManager="*res://common/World/ZoneManager.gd"
CharacterService="*res://autoload/CharacterService.gd"
FailureLogger="*res://common/Test/FailureLogger.gd"
ComponentTracker="*res://common/Test/ComponentTracker.gd"
TDDConfig="*res://common/Test/TDDConfig.gd"
RuntimeErrorAggregator="*res://common/Test/RuntimeErrorAggregator.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_local_constant=false
gdscript/warnings/unused_private_class_variable=false
gdscript/warnings/unused_parameter=false
gdscript/warnings/unused_signal=false
gdscript/warnings/shadowed_variable=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"

[gdscript]

warnings/shadowed_variable=true
warnings/int_as_enum_without_cast=true
warnings/redundant_await=true
warnings/untyped_declaration=0
warnings/inferred_declaration=0
warnings/unsafe_property_access=0
warnings/unsafe_method_access=0
warnings/unsafe_cast=0
warnings/unsafe_call_argument=0

